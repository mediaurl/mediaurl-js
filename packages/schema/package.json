{
  "name": "@watchedcom/schema",
  "description": "WATCHED.com JSON Schema",
  "version": "0.19.3",
  "main": "dist",
  "scripts": {
    "prepublishOnly": "npm run build",
    "build": "rm -rf dist && mkdir -p dist && npm run build:tsc && npm run build:schema && npm run build:types",
    "build:tsc": "tsc",
    "build:schema": "node ./buildSchema.js",
    "build:types": "node ./buildTypes.js",
    "test": "jest"
  },
  "dependencies": {
    "ajv": "^6.12.0"
  },
  "devDependencies": {
    "@types/jest": "^25.1.4",
    "@types/node": "^13.9.0",
    "jest": "^25.1.0",
    "js-yaml": "^3.13.1",
    "json-schema-ref-parser": "^7.1.4",
    "json-schema-to-typescript": "^8.1.0",
    "ts-jest": "^25.2.1",
    "typescript": "^3.8.3"
  }
}
